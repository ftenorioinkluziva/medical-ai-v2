# Database Configuration
# IMPORTANTE: Use a MESMA DATABASE_URL do seu arquivo .env local
DATABASE_URL=your_database_url_from_local_env

# NextAuth Configuration
# Gere um novo secret com: openssl rand -base64 32
NEXTAUTH_SECRET=generate_with_openssl_rand_base64_32
NEXTAUTH_URL=https://your-domain.com

# AI API Keys
# IMPORTANTE: Use as MESMAS chaves do seu arquivo .env local
GOOGLE_GENERATIVE_AI_API_KEY=your_google_ai_api_key_from_local_env
OPENAI_API_KEY=your_openai_api_key_from_local_env

# Application
NODE_ENV=production
PORT=3000
