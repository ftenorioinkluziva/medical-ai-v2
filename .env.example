  # .env.example
  # Database
  DATABASE_URL="postgresql://user:password@localhost:5432/medical_ai_v2"

  # Google AI (Gemini) - REQUIRED
  # Used for: text generation, embeddings, vision/OCR
  GOOGLE_GENERATIVE_AI_API_KEY="your_google_ai_key"

  # OpenAI - OPTIONAL (legacy support only)
  # Not required for system operation
  # OPENAI_API_KEY="your_openai_key"

  # NextAuth
  NEXTAUTH_SECRET="your_secret_here" # gerar com: openssl rand -base64 32
  NEXTAUTH_URL="http://localhost:3000"

  # Stripe (Billing System)
  STRIPE_SECRET_KEY="sk_test_..." # Get from Stripe Dashboard
  STRIPE_PUBLISHABLE_KEY="pk_test_..." # Get from Stripe Dashboard
  STRIPE_WEBHOOK_SECRET="whsec_..." # Get from Stripe CLI or Dashboard
  NEXT_PUBLIC_APP_URL="http://localhost:3000" # Your app URL for Stripe redirects

  # Email (opcional para produção)
  EMAIL_FROM="noreply@yourdomain.com"
  SMTP_HOST=""
  SMTP_PORT="587"
  SMTP_USER=""
  SMTP_PASS=""